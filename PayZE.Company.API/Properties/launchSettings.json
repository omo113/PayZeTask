{
  "profiles": {
    "PayZE.Company.Api": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "settings__RabbitMqHost": "localhost",
        "settings__RabbitMqUser": "admin",
        "settings__RabbitMqPassword": "admin",
        "settings__DatabaseConnection": "Host=localhost; Database=dotnet-PayZe-Identity; Username=myuser; Password=mypassword;Pooling=true;",
        "settings__RedisConnectionString": "localhost:6379"
      },
      "applicationUrl": "https://localhost:7259"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json"
}